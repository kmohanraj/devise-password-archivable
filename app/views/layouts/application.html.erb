<!DOCTYPE html>
<html>
  <head>
    <title>DevisePasswordArchivable</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if user_signed_in? %>
      <%= current_user.email %> | <%= link_to "Logout", destroy_user_session_path, method: :delete %> | <%= link_to "Edit Profile", edit_user_registration_path %>
    <% end %>
    <%= yield %>
  </body>
</html>
